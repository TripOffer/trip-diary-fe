# 环境变量示例文件
# 复制为 .env 并根据实际情况填写

# Vite/Tauri 相关端口
VITE_TAURI_DEV_PORT=1420
VITE_TAURI_DEV_HMR_PORT=1421
VITE_TAURI_DEV_HOST=localhost

# API 相关配置
# 这里的 API_BASE_URL 是一个示例，实际使用时请根据你的 API 地址进行修改
VITE_API_BASE_URL=https://api.example.com
VITE_API_TIMEOUT=30000

# 其他配置
VITE_APP_NAME=Trip Diary
VITE_APP_NAME_EN_US=Trip Diary
VITE_APP_NAME_ZH_CN=旅行日记

VITE_APP_VERSION=0.1.0

# OSS 相关配置
VITE_OSS_URL=https://oss.example.com